{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=1 eslint .",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vitest",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "scan": "ast-grep --config ../../scripts/ast-grep.yml apps/web",
    "dead": "knip --include unused --workspace --config ../../knip.json",
    "dupes": "jscpd --pattern 'apps/web/**/*.{ts,tsx}' --threshold 2",
    "refactor:check": "pnpm run scan && pnpm run dead && pnpm run dupes"
  },
  "dependencies": {
    "@breathe-mail/email": "workspace:*",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.441.0",
    "next": "^14.2.13",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.17",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.10",
    "@types/jsdom": "^21.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  }
}
